<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib"
                    xml:space="preserve" >

    <system:String x:Key="CurrentFileName">Name</system:String>
    <system:String x:Key="CurrentFileTitle">Title</system:String>
    <system:String x:Key="CurrentFilePages">Pages</system:String>
